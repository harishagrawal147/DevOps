    "def string_copies(s, n):\n",
    "    if n < 0:\n",
    "        return \"Error: n must be a non-negative integer.\"\n",
    "    return s * n\n",
    "\n",
    "text = \"Hello\"\n",
    "n = 3\n",
    "result = string_copies(text, n)\n",
    "print(result)"
