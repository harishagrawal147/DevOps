    "def new_string(s):\n",
    "    if s.startswith(\"Is\"):\n",
    "        return s\n",
    "    else:\n",
    "        return \"Is\" + s\n",
    "\n",
    "print(new_string(\"Harish\"))  \n",
    "print(new_string(\"IsHarsh\"))"
