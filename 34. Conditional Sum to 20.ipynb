    "def custom_sum(a, b):\n",
    "    total = a + b\n",
    "    if 15 <= total <= 20:\n",
    "        return 20\n",
    "    else:\n",
    "        return total\n",
    "\n",
    "x = int(input(\"Enter first integer: \"))\n",
    "y = int(input(\"Enter second integer: \"))\n",
    "\n",
    "print(f\"The result is: {custom_sum(x, y)}\")"
   ]
