   "source": [
    "def add_if_integers(a, b):\n",
    "    if isinstance(a, int) and isinstance(b, int):\n",
    "        return a + b\n",
    "    else:\n",
    "        return \"Both objects must be integers.\"\n",
    "\n",
    "x = 10\n",
    "y = 20\n",
    "print(add_if_integers(x, y))   \n",
    "\n",
    "x = 10\n",
    "y = \"20\"\n",
    "print(add_if_integers(x, y))   "
   ]
