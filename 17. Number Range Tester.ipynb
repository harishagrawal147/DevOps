   "source": [
    "n = int(input(\"Enter a number: \"))\n",
    "\n",
    "if abs(1000 - n) <= 100 or abs(2000 - n) <= 100:\n",
    "    print(\"Number is within 100 of 1000 or 2000.\")\n",
    "else:\n",
    "    print(\"Number is not within 100 of 1000 or 2000.\")"
