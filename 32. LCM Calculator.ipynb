   "source": [
    "import math\n",
    "\n",
    "def compute_lcm(a, b):\n",
    "    return abs(a * b) // math.gcd(a, b)\n",
    "\n",
    "x = int(input(\"Enter first positive integer: \"))\n",
    "y = int(input(\"Enter second positive integer: \"))\n",
    "\n",
    "print(f\"The LCM of {x} and {y} is: {compute_lcm(x, y)}\")"
   ]
