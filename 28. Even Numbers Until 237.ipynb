    "def print_even_until_237(numbers):\n",
    "    for num in numbers:\n",
    "        if num == 237:\n",
    "            break\n",
    "        if num % 2 == 0:\n",
    "            print(num)\n",
    "\n",
    "my_list = [\n",
    "    12, 75, 150, 180, 145, 237, 250, 300, 400\n",
    "]\n",
    "\n",
    "print_even_until_237(my_list)"
